{
  "name": "soluva-orchestrator",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --transpile-only --require tsconfig-paths/register --require dotenv/config src/index.ts",
    "start": "node dist/index.js",
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "test": "jest --setupFiles dotenv/config",
    "lint": "eslint . --quiet",
    "trends:calculate": "tsx scripts/calculate_trends.ts",
    "clusters:recompute": "tsx scripts/recompute-clusters.ts",
    "generate-types": "npx supabase gen types typescript --project-id ppgtklxuvzepdnamownm --schema public > src/types/database.ts"
  },
  "dependencies": {
    "@langchain/openai": "^0.2.11",
    "@supabase/supabase-js": "^2.45.0",
    "bullmq": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "joi": "^17.13.3",
    "mongoose": "^8.18.1",
    "openai": "^4.67.1",
    "p-limit": "^5.0.0",
    "pino": "^8.16.2",
    "prom-client": "^15.1.0",
    "tiktoken": "^1.0.22",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@soluva/eslint-config": "workspace:*",
    "@soluva/types": "workspace:*",
    "@soluva/typescript-config": "workspace:*",
    "@swc/core": "^1.13.5",
    "@swc/helpers": "^0.5.17",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.10.0",
    "dotenv": "^16.3.1",
    "eslint": "^9.34.0",
    "jest": "^30.1.3",
    "supabase": "^2.40.7",
    "ts-jest": "^29.4.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "tsup": "6.6.3",
    "typescript": "5.9.2"
  }
}